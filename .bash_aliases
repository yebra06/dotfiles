# Bash
alias ls='ls -GFh'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

# Git
alias st='git status'
alias gd='git diff'
alias fetch='git fetch'
alias rebase='rebase -i'

# Virtualenvs
alias venv2='mkvirtualenv'
alias venv3='mkvirtualenv --python=/usr/local/bin/python3'

# Django
alias runserver='python manage.py runserver'
